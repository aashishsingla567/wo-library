{
  "name": "wo-library",
  "version": "0.1.0",
  "type": "module",
  "engines": {
    "node": "^14.13.1 || >=16.0.0"
  },
  "description": "A component library smooth on the edges",
  "keywords": [
    "wo-library",
    "components",
    "react"
  ],
  "exports": {
    "./components/atoms/*": "./components/atoms/*/index.ts",
    "./components": "./components/index.js",
    "./contexts": "./contexts/index.ts",
    "./hooks": "./hooks/index.ts",
    "./lib/cjs": {
      "require": "./lib/cjs/index.cjs"
    },
    "./lib/*": "./lib/*/index.ts",
    "./lib": "./lib/index.ts",
    "./utils/*": "./utils/*"
  },
  "scripts": {
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@floating-ui/react-dom-interactions": "^0.3.1",
    "@react-native-async-storage/async-storage": "^1.17.3",
    "clsx": "^1.1.1",
    "http-proxy-middleware": "^2.0.6",
    "immer": "^9.0.12",
    "lodash-es": "^4.17.21",
    "micromatch": "^4.0.5",
    "paths.macro": "^3.0.1",
    "prop-types": "^15.8.1",
    "react-hook-form": "^7.30.0",
    "react-icons": "^4.3.1",
    "react-intersection-observer": "^9.0.0",
    "react-use": "^17.3.2",
    "swiper": "^8.1.3",
    "ye-ui": "workspace:*"
  },
  "devDependencies": {
    "@mdx-js/react": "^2.1.1",
    "@storybook/addon-docs": "^6.4.22",
    "@storybook/addon-links": "^6.4.22",
    "@testing-library/react": "^13.1.1",
    "@types/node": "^17.0.25",
    "@types/react": "^18.0.5",
    "@types/react-dom": "^18.0.1"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "workspaces": [
    "packages/*"
  ]
}
